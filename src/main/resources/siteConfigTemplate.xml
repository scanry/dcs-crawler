<siteProfile>
	<!--站点code -->
	<site code="tmsf">
		<!--站点主页-->
		<mainUrl>http://www.tmsf.com</mainUrl>
		<!--站点描述-->
		<describe>杭州透明售房网</describe>
	</site>

	<!--  name:路径名字
		  siteCode:站点code
		   ranking:路径使用排名
    extractAttName:路径要获取的值的属性名字  
		appendHead:在头部追加字符  
		 appendEnd:在尾部追加字符 
	compareAttName:用来比较 的att name  
	containKeyWord:包含的关键字   
	substringStart:截止字符开始字符，不设值 默认从头开始
	  substringEnd:截取字符结尾字符,不设值 默认截取到尾部  
	   replaceWord:要替换的词  
	  replaceValue:替换的值
	          path:要抽取的路径path
		filterPath:需要过滤的路径path
		  describe:描述-->
		<extractPath name="projectName" siteCode="tmsf" ranking="1" extractAttName="text" appendHead="" appendEnd="" compareAttName="" containKeyWord="">
			<path>div[class=lpxqtop]>div>div>span[class=buidname colordg]</path>
			<filterPath></filterPath>
			<substringStart></substringStart>
			<substringEnd></substringEnd>
			<replaceWord></replaceWord>
			<replaceValue></replaceValue>
			<describe>项目名称</describe>
		</extractPath>
		<extractPath name="brandName" siteCode="tmsf" ranking="1" extractAttName="text" appendHead="" appendEnd="" compareAttName="" containKeyWord="" >
			<path>div[class=lpxqtop]>div>div>span[class=extension famwei ft14 mgr10]>ul>li:eq(1)</path>
			<filterPath></filterPath>
			<replaceWord></replaceWord>
			<replaceValue></replaceValue>
			<describe>推广名称</describe>
		</extractPath>
		<extractPath name="propertyType" siteCode="tmsf" ranking="1" extractAttName="text" appendHead="" appendEnd="" compareAttName="" containKeyWord="">
			<path>div[class=buildtxtbox classones fl famwei]>ul>li:contains(物业类型)</path>
			<filterPath></filterPath>
			<replaceWord>物业类型：</replaceWord>
			<replaceValue></replaceValue>
			<describe>物业类型</describe>
		</extractPath>
		<extractPath name="district" siteCode="tmsf" ranking="1" extractAttName="text" appendHead="" appendEnd="" compareAttName="" containKeyWord="">
			<path>div[class=website colormg]>a[id=districtname]</path>
